{
    "elements": [
      {
      	"name": "Carousel",
      	"handle": "carousel",
      	"customizations": "box",
      	"hoverCustomizations": true,
      },
      {
      	"name": "Carousel Image",
      	"handle": "carousel-image",
      	"customizations": "box",
      	"hoverCustomizations": true,
        "activeCustomizations": true,
      }
    ],
	"properties"	: [
		{
			"name": "Images",
	    		"variable": "images",
	    		"type": "image-gallery",
	    		"default": [
				[
    "https://yawb-user-images.s3.amazonaws.com/user-images/65413ccaf37ccc1e17ab83da/apple-65414d348a3c156ebb03c274.png",
    "https://yawb-user-images.s3.amazonaws.com/user-images/65413ccaf37ccc1e17ab83da/aws-65414d348a3c15576003c273.png",
    "https://yawb-user-images.s3.amazonaws.com/user-images/65413ccaf37ccc1e17ab83da/ferero-65414d348a3c155eb103c272.png",
    "https://yawb-user-images.s3.amazonaws.com/user-images/65413ccaf37ccc1e17ab83da/airbus-65414d348a3c15668e03c271.png",
    "https://yawb-user-images.s3.amazonaws.com/user-images/65413ccaf37ccc1e17ab83da/microsoft-65414d358a3c15cb6c03c276.png",
    "https://yawb-user-images.s3.amazonaws.com/user-images/65413ccaf37ccc1e17ab83da/google-65414d348a3c1539c103c275.png",
    "https://yawb-user-images.s3.amazonaws.com/user-images/65413ccaf37ccc1e17ab83da/tesla-65414d368a3c151ca103c277.png"
]
			]
		},
        {
			"name"			: "Direction",
			"variable"		: "direction",
			"type"			: "dropdown",
            "options"		: [
              {
                "name":"Left",
                "value":"left"
              },
              {
                "name":"Right",
                "value":"right"
              }
            ],
			"default"		: "left"
		},
        {
			"name"			: "Speed",
			"variable"		: "speed",
			"type"			: "slider",
            "cssVariable"	: true,
            "min"			: 0.5,
            "max"			: 10,
            "unit"			: "s",
			"default"		: 2
		},
        {
			"name"			: "Image Width",
			"variable"		: "imageWidth",
			"type"			: "slider",
            "cssVariable"	: true,
            "min"			: 30,
            "max"			: 500,
            "integer"		: true,
            "unit"			: "px",
			"default"		: 160
		},
        {
			"name"			: "Spacing",
			"variable"		: "spacing",
			"type"			: "slider",
            "cssVariable"	: true,
            "min"			: 0,
            "max"			: 300,
            "integer"		: true,
            "unit"			: "px",
			"default"		: 80
		},
        {
			"name"			: "Link (optional)",
			"variable"		: "url",
			"type"			: "url"
		}
	]
}
