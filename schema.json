{
    "elements": [
      {
      	"name": "Carousel",
      	"handle": "carousel",
      	"customizations": "box",
      	"hoverCustomizations": true,
      },
      {
      	"name": "Carousel Image",
      	"handle": "carousel-image",
      	"customizations": "box",
      	"hoverCustomizations": true,
        "activeCustomizations": true,
      }
    ],
	"properties"	: [
		{
			"name": "Images",
	    		"variable": "images",
	    		"type": "image-gallery",
	    		"default": [
				'https://yawb-user-images.s3.amazonaws.com/user-ima…7ccc1e17ab83da/apple-65414c298a3c15cfca03c261.png', 'https://yawb-user-images.s3.amazonaws.com/user-ima…ccc1e17ab83da/ferero-65414c298a3c15070303c260.png', 'https://yawb-user-images.s3.amazonaws.com/user-ima…ccc1e17ab83da/google-65414c298a3c15848403c262.png', 'https://yawb-user-images.s3.amazonaws.com/user-ima…1e17ab83da/microsoft-65414c2a8a3c15107703c263.png', 'https://yawb-user-images.s3.amazonaws.com/user-ima…ccc1e17ab83da/airbus-65414c298a3c15746b03c25f.png', 'https://yawb-user-images.s3.amazonaws.com/user-ima…7ccc1e17ab83da/tesla-65414c2b8a3c1530d803c264.png', 'https://yawb-user-images.s3.amazonaws.com/user-ima…f37ccc1e17ab83da/aws-65414c308a3c15db6803c265.png'
			]
		},
        {
			"name"			: "Direction",
			"variable"		: "direction",
			"type"			: "dropdown",
            "options"		: [
              {
                "name":"Left",
                "value":"left"
              },
              {
                "name":"Right",
                "value":"right"
              }
            ],
			"default"		: "left"
		},
        {
			"name"			: "Speed",
			"variable"		: "speed",
			"type"			: "slider",
            "cssVariable"	: true,
            "min"			: 0.5,
            "max"			: 10,
            "unit"			: "s",
			"default"		: 2
		},
        {
			"name"			: "Image Width",
			"variable"		: "imageWidth",
			"type"			: "slider",
            "cssVariable"	: true,
            "min"			: 30,
            "max"			: 500,
            "integer"		: true,
            "unit"			: "px",
			"default"		: 160
		},
        {
			"name"			: "Spacing",
			"variable"		: "spacing",
			"type"			: "slider",
            "cssVariable"	: true,
            "min"			: 0,
            "max"			: 300,
            "integer"		: true,
            "unit"			: "px",
			"default"		: 80
		},
        {
			"name"			: "Link (optional)",
			"variable"		: "url",
			"type"			: "url"
		}
	]
}
